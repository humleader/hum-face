{
  "apps": [
    {
      "name": "hum-face-staging",
      "script": "./server/server.js",
      "instances": "2",
      "exec_mode": "cluster",
      "max_memory_restart": "500M",
      "env": {
        "NODE_ENV": "staging",
        "BLUEBIRD_WARNINGS": 0
      }
    },
    {
      "name": "hum-face-production",
      "script": "./server/server.js",
      "instances": "4",
      "exec_mode": "cluster",
      "max_memory_restart": "500M",
      "env": {
        "NODE_ENV": "production",
        "BLUEBIRD_WARNINGS": 0
      }
    }
  ]
}
